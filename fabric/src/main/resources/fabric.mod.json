{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "Modularized Vanilla++ mod for Minecraft. Includes crystals, experience management, extras, commands, enchantments, and more.",
  "authors": [
    "ShkSchneider"
  ],
  "contact": {
    "homepage": "https://github.com/shkschneider/mc_awesome",
    "sources": "https://github.com/shkschneider/mc_awesome"
  },
  "license": "MIT",
  "icon": "assets/awesome/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "io.github.shkschneider.awesome.fabric.AwesomeFabric"
      }
    ],
    "emi": [
      "io.github.shkschneider.awesome.crystals.AwesomeEmiPlugin"
    ]
  },
  "mixins": [
    "awesome.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.11",
    "fabric-api": "*",
    "minecraft": "~1.20.6",
    "java": ">=21",
    "fabric-language-kotlin": ">=1.10.20+kotlin.1.9.24",
    "architectury": ">=12.0.0"
  },
  "suggests": {
    "another-mod": "*"
  }
}
